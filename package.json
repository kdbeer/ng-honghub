{
  "name": "ng-honghub",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-qa": "ng build --prod --aot --build-optimizer",
    "build-dev": "ng build -c dev",
    "build-sit": "ng build -c sit",
    "build-prod": "ng build -c production",
    "build-stats": "ng build --prod --aot --build-optimizer --stats-json",
    "bundle-analyse": "webpack-bundle-analyzer dist/stats.json",
    "bundle-stats-analyse": "npm run build-stats && npm run bundle-analyse",
    "build-aws": "npm run build-prod &&  aws s3 cp ./dist s3://ng-honghub/ --recursive --acl public-read --region ap-southeast-1",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngc": "ngc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@ngrx/effects": "^7.0.0-beta.0",
    "@ngrx/entity": "^7.0.0-beta.0",
    "@ngrx/store": "^7.0.0-beta.0",
    "@ngrx/store-devtools": "^7.0.0-beta.0",
    "@swimlane/ngx-datatable": "^14.0.0",
    "core-js": "^2.5.7",
    "ng-pick-datetime": "^7.0.0",
    "node-sass": "^4.12.0",
    "npm-check-updates": "^3.1.9",
    "rxjs": "~6.5.2",
    "skeleton-css": "^2.0.4",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.2",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@ngrx/schematics": "^6.1.2",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.10",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
